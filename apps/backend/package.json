    "deploy": "wrangler deploy src/index.ts",
    "start": "wrangler dev",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.0.12",
    "@aws-sdk/client-s3": "^3.717.0",
