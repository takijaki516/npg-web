    "@hono/zod-validator": "^0.4.2",
    "@repo/db": "workspace:*",
    "@repo/utils": "workspace:*",
    "@repo/shared-schema": "workspace:*",
    "ai": "^4.0.18",
    "better-auth": "^1.1.4",
    "drizzle-orm": "^0.38.3",
    "hono": "^4.6.15",
    "luxon": "^3.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241224.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "20.8.3",
    "@types/service-worker-mock": "^2.0.4",
    "typescript": "~5.6.3",
    "wrangler": "^3.99.0"
