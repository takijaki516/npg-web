{
  "name": "@repo/backend",
  "exports": {
    "./hc": {
      "import": {
        "default": "./dist/hc.js",
        "types": "./dist/hc.d.ts"
      }
    }
  },
  "scripts": {
    "dev": "pnpm --filter @repo/backend --parallel \"/^dev:.*/\"",
    "dev:tsc": "tsc -p tsconfig.build.json --watch",
    "dev:wrangler": "wrangler dev src/index.ts",
